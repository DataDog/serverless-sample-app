{
  "name": "node-lambda-hybrid-tracing",
  "version": "0.1.0",
  "type": "commonjs",
  "bin": {
    "pricing-service": "bin/pricing-service.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test": "jest",
    "cdk": "cdk",
    "dev:sst": "sst dev --stage=personal",
    "build:sst": "sst build --stage=dev",
    "deploy:sst": "sst deploy --stage=dev",
    "remove:sst:personal": "sst remove --stage=personal",
    "remove:sst": "sst remove --stage=dev",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@aws-sdk/client-eventbridge": "^3.772.0",
    "@aws-sdk/client-ssm": "^3.772.0",
    "@types/aws-lambda": "8.10.148",
    "@types/jest": "^29.5.14",
    "@types/node": "22.14.0",
    "aws-cdk": "2.1007.0",
    "aws-cdk-lib": "^2.185.0",
    "axios": "^1.8.4",
    "constructs": "^10.4.2",
    "datadog-cdk-constructs-v2": "^2.2.0",
    "datadog-lambda-js": "^10.124.0",
    "dd-trace": "^5.43.0",
    "esbuild": "^0.20.2",
    "jest": "^29.7.0",
    "moq.ts": "^10.1.0",
    "serverless-esbuild": "^1.54.6",
    "serverless-plugin-datadog": "^5.77.0",
    "serverless-step-functions": "^3.21.2",
    "source-map-support": "^0.5.21",
    "sst": "^2.45.1",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.17.0",
    "@aws-lambda-powertools/parameters": "^2.17.0",
    "@types/jsonwebtoken": "^9.0.9",
    "cloudevents": "^8.0.3",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.24.2"
  }
}
